                    </div>
                </div>

            </div>



            <div  id="wholecontent" class="dashboard_screen_views dashboard_right_ship">

                {% block body %}

                {% endblock %}

            </div>


        </div>
    </div>

    <!-- end if for overall links -->
    {% for type, messages in app.session.flashBag.all %}
        {% for message in messages %}
            <div class="{{ type }}">
                {{ message|trans({}, 'FOSUserBundle') }}
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}
<div>
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>
