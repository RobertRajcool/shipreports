                                    <span class="menu-item-text" id="Scorecard">{{ 'layout.menu.scorecard'|trans }}</span>
                                    <span class="x-btn-icon-arrow"><img src="/images/arrow-buttom.png"></span>
                                </a>

                            </li>

                            <li class="topmenu_ranking" id="Vessel_Ranking1">
                                <a class="layout_name rankinglink" id="Vessel_Ranking" ng-click="myFuncVesselRanking()">
                                    <span class="menu-item-icon ss-warehouse">&nbsp;</span>
                                    <span class="menu-item-text layout_name">{{ 'layout.menu.vesselranking'|trans }}</span>
                                    <span class="x-btn-icon-arrow"><img src="/images/arrow-buttom.png"></span>
                                </a>

                            </li>
                        {% endif %}
                    {% endif %}
                    {%  if is_granted("ROLE_ADMIN") %}
                        <li id="fos_user_registration_show21">
                            <a class="layout_name" id="fos_user_registration_show" href="{{ path('fos_user_registration_show') }}">
                                <span class="menu-item-icon ss-users">&nbsp;</span>
                                <span class="menu-item-text">{{ 'layout.menu.user'|trans }}</span>
                            </a>
                        </li>
                    {% endif %}
                    {%  if is_granted("ROLE_SUPER_ADMIN") %}
                        <li id="fos_user_registration_show1">
                            <a class="layout_name" id="fos_user_registration_show" href="{{ path('fos_user_registration_show') }}">
                                <span class="menu-item-icon ss-users">&nbsp;</span>
                                <span class="menu-item-text">{{ 'layout.menu.companydetails'|trans }}</span>
                            </a>
                        </li>
                    {% endif %}
                    {%  if is_granted("ROLE_MANAGER") or is_granted("ROLE_ADMIN") %}
                        <li id="creategroup1">
                            <a class="layout_name" id="creategroup" href="{{ path('creategroup') }}">
                                <span class="menu-item-icon ss-mail">&nbsp;</span>
                                <span class="menu-item-text">{{ 'layout.menu.mail'|trans }}</span>
                            </a>
                        </li>
                    {% endif %}
                    <li id="backuplist">
                        <a href="{{ path('database_export') }}" target="">
                            <span class="menu-item-icon ss-briefcase">&nbsp;</span>
                            <span class="menu-item-text">{{ 'layout.menu.backup'|trans }}</span>
                        </a>
                    </li>
                </ul>


                <div class="x-btn-my-account-small" id="bottom_content">
                    <div class="x-btn-my-account-small_inner_2">
                        <div id="slidetoggleid" class="user_setting_main slide-toggle">

    <span class="x-btn-icon-el" id="user_settings">
    <img src="/images/nouser.png">
    </span>
    <span class="x-btn-inner">
    <div class="user-name">{{app.user.username }}</div>
    <div class="company">
        {% if app.user.roles[0]=='ROLE_ADMIN' %}
            Administrator
        {% elseif app.user.roles[0]=='ROLE_MANAGER' %}
            Manager
        {% elseif app.user.roles[0]=='ROLE_KPI_INFO_PROVIDER' %}
            V-ship
        {% else %}
            User
        {% endif %}
