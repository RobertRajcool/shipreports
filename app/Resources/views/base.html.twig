                    $('<li id="kpidetails_select1"><a class="layout_name" id="kpidetails_select" href="{{ path('kpidetails_select1') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text " id="kpidetails_select">{{ 'layout.menu.addkpi'|trans }}</span></a></li><li id="elementdetails_select1"><a class="layout_name" id="elementdetails_select" href="{{ path('elementdetails_select1') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text" id="elementdetails_select">{{ 'layout.menu.element'|trans }}</span></a></li><li id="rules_select1"><a class="layout_name" id="rules_select" href="{{ path('rules_select') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text" id="rules_select">{{ 'layout.menu.rule'|trans }}</span></a></li><li id="data_verification1"><a class="layout_name" id="data_verification" href="{{ path('adddata_scorecard') }}"><span class="menu-item-icon ss-check">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.dv'|trans }}</span></a></li><li id="reports1"><a class="layout_name" id="reports" href="{{ path('scorecard_report_home') }}"><span class="menu-item-icon ss-barchart">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.report'|trans }}</span></a></li>').appendTo('#scorecard_submenu');
                    $('.scorecard').show();
                }
            });
            $(".topmenu_ranking").click(function()
            {
                $('.scorecard').hide();
                $(".box_new").hide();
                if($('#ranking_submenu').length > 0)
                {
                    $('#ranking_submenu').remove();
                }
                else
                {
                    $('.rankinglink').after('<ul class="submenu ranking_submenu" id="ranking_submenu"></ul>');
                    $('#ranking_submenu').html('');
                    $('#ranking_submenu').text('');
                    $('<li id="rankingkpidetails_select1"><a class="layout_name" id="rankingkpidetails_select" href="{{ path('rankingkpidetails_select') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.addkpi'|trans }}</span></a></li><li id="rankingelementdetails_select1"><a class="layout_name" id="rankingelementdetails_select" href="{{ path('rankingelementdetails_select1') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.element'|trans }}</span></a></li><li id="rankingrules_select1"><a class="layout_name" id="rankingrules_select1" href="{{ path('rankingrules_select') }}"><span class="menu-item-icon ss-plus">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.rule'|trans }}</span></a></li><li id="ranking_data_verification1"><a class="layout_name" id="ranking_data_verification1" href="{{ path('adddata_scorecard_forranking') }}"><span class="menu-item-icon ss-check">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.dv'|trans }}</span></a></li><li  id="Ranking_Reports1"><a id="Ranking_Reports" class="layout_name" href="{{ path('ranking_reports') }}"><span class="menu-item-icon ss-barchart">&nbsp;</span><span class="menu-item-text">{{ 'layout.menu.report'|trans }}</span></a></li>').appendTo('#ranking_submenu');
                    $(".ranking_submenu").show();
                }

            });
            $(".slide-toggle").click(function()
            {
                $('.scorecard').hide();
                $('.ranking_submenu').hide()
                if($('#user_login_content').length > 0)
                {
                    $(".box_new").slideToggle();
                    //$('#user_login_content').remove();
                }
                else
                {
                    $('#user_settings').before('<div class="profile_list box_new" id="user_login_content"><ul><li><a href={{ path('fos_user_change_password') }}>{{ 'layout.changepass'|trans }}</a></li><li class="last"><a href="{{ path('fos_user_security_logout') }}">{{ 'layout.logout'|trans }}</a></li><li><a href="{{ path('fos_user_edit_profile') }}" id=""> Edit Profile</a></li></ul></div>');
                    //$(".box_new").fadeIn('slow');
                    $('.box_new').hide();
                    $(".box_new").slideToggle();
                }

            });
            $(document).click(function (e) {
                if (!$(e.target).parents().andSelf().is('.slide-toggle'))
                {
                    $('#user_login_content').remove();
                }
            });

        });
    </script>
</head>
<body>
{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
    <div class="container" >
        <div class="row">
            <div  class="col-md-2 left_menus-2" id="afterlogin_appendcontent">
                <div class="logo"><img src="/images/pioneer_logo.png"></div>
