{% extends 'base.html.twig' %}

{% block body %}
    <h1>AppsCountries</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appsCountry.id }}</td>
            </tr>
            <tr>
                <th>Countrycode</th>
                <td>{{ appsCountry.countryCode }}</td>
            </tr>
            <tr>
                <th>Countryname</th>
                <td>{{ appsCountry.countryName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('appscountries_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('appscountries_edit', { 'id': appsCountry.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
