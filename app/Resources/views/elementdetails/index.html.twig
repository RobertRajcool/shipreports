                            type:"POST",
                            url:"elements_for_kpi",/*Routing.generate('elements_for_kpi'),*/
                            data:data,
                            beforeSend: function () {
                                var setAjaxBeforeSend = defaultAjaxBeforeSendFunction();
                            },
                            complete: function () {
                                var setAjaxComplete = defaultAjaxCompleteFunction();
                            }
