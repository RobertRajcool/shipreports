                                editSymbolIndicationClass.val(data.elementSymbolDetail[0].symbolIndication);
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown) {
                                alert('Error : ' + errorThrown);
                            }
                        });
                    }
                });

                var editSymbolSaveBtn = $('#symbol-save-id');
                var editSymbolDeleteBtn = $('#symbol-delete-id');
                editSymbolSaveBtn.click(function($e) {
                   if(symbolEditSelectClass.val() == 0) {
                       alert('Select symbol');
                   } else if( !$.trim(editSymbolNameClass.val()) && !$.trim(editSymbolIndicationClass.val()) ) {
                       alert('All fields are required')
                   } else {
                       var data = {
                           symbolId: symbolEditSelectClass.val(),
                           symbolName: editSymbolNameClass.val(),
                           symbolIndication: editSymbolIndicationClass.val()
                       };
                       $.ajaxSetup({
                           global: false,
                           type:"POST",
                           url:"save_element_symbol",/*Routing.generate('save_element_symbol'),*/
                           data:data,
                           beforeSend: function () {
                               var setAjaxBeforeSend = defaultAjaxBeforeSendFunction();
                           },
                           complete: function () {
                               var setAjaxComplete = defaultAjaxCompleteFunction();
                           }
                       });
                       $.ajax({
                           success: function(data) {
                               editSymbolNameClass.val('');
                               editSymbolIndicationClass.val('');
                               $('#symbol-edit-option-id option:gt(0)').remove();
                               $.each(data.elementSymbolDetail, function(i, symbol) {
                                   $('#symbol-edit-option-id').append($('<option>', {
                                       value: symbol.id, text: symbol.symbolName
                                   }));
                               });
                               $('#element_details_SymbolId option:gt(0)').remove();
                               $.each(data.elementSymbolDetail, function(i, symbol) {
                                   $('#element_details_SymbolId').append($('<option>', {
                                       value: symbol.id, text: symbol.symbolName
                                   }));
                               });
                           },
                           error: function(XMLHttpRequest, textStatus, errorThrown) {
                               alert('Error : ' + errorThrown);
                           }
                       });
                   }
                });
                editSymbolDeleteBtn.click(function($e) {
                    if(symbolEditSelectClass.val() == 0) {
                        alert('Select symbol');
                    } else {
                        var data = {
                            symbolId: symbolEditSelectClass.val()
                        };
                        $.ajaxSetup({
                            global: false,
                            type:"POST",
                            url:"delete_element_symbol",/*Routing.generate('delete_element_symbol'),*/
                            data:data,
                            beforeSend: function () {
                                var setAjaxBeforeSend = defaultAjaxBeforeSendFunction();
                            },
                            complete: function () {
                                var setAjaxComplete = defaultAjaxCompleteFunction();
                            }
                        });
                        $.ajax({
                            success: function(data) {
                                editSymbolNameClass.val('');
                                editSymbolIndicationClass.val('');
                                $('#symbol-edit-option-id option:gt(0)').remove();
                                $.each(data.elementSymbolDetail, function(i, symbol) {
                                    $('#symbol-edit-option-id').append($('<option>', {
                                        value: symbol.id, text: symbol.symbolName
                                    }));
                                });
                                $('#element_details_SymbolId option:gt(0)').remove();
                                $.each(data.elementSymbolDetail, function(i, symbol) {
                                    $('#element_details_SymbolId').append($('<option>', {
                                        value: symbol.id, text: symbol.symbolName
                                    }));
                                });
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown) {
                                alert('Error : ' + errorThrown);
                            }
                        });
                    }
                });


            });
        </script>

