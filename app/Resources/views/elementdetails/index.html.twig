                                $('#symbol-edit-option-id').append($('<option>', {
                                    value: symbol.id, text: symbol.symbolName
                                }));
                            });
                        },
                        error: function(XMLHttpRequest, textStatus, errorThrown) {
                            alert('Error : ' + errorThrown);
                        }
                    });
                });

                var editSymbolNameClass = $('.edit-symbol-name-class');
                var editSymbolIndicationClass = $('.edit-symbol-indication-class');
                var symbolEditSelectClass = $('.symbol-edit-option');
                symbolEditSelectClass.live('change',function($e) {
                    if(symbolEditSelectClass.val()==0) {
                        alert('Select symbol');
                    } else {
                        var data = {
                            symbolId: symbolEditSelectClass.val()
                        };
                        $.ajaxSetup({
                            global: false,
                            type:"POST",
                            url:"find_element_symbol_name",/*Routing.generate('find_element_symbol_name'),*/
                            data:data,
                            beforeSend: function () {
                                var setAjaxBeforeSend = defaultAjaxBeforeSendFunction();
                            },
                            complete: function () {
                                var setAjaxComplete = defaultAjaxCompleteFunction();
                            }
                        });
                        $.ajax({
