                            var setAjaxBeforeSend = defaultAjaxBeforeSendFunction();
                        },
                        complete: function () {
                            var setAjaxComplete = defaultAjaxCompleteFunction();
                        }
                    });
                    $.ajax({
                        success: function(data) {
                            editSymbolTextPlace.hide();
                            $('#symbol-edit-option-id option:gt(0)').remove();
