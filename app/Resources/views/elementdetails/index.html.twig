                function setOptionSymbolFunction(symbol) {
                    if(symbol=='equalTo') {
                        return '='
                    } else if(symbol=='greaterThan') {
                        return '>'
                    } else if(symbol=='lessThan') {
                        return '<'
                    } else if(symbol=='greaterThanEqual') {
                        return '>='
                    } else if(symbol=='lessThanEqual') {
                        return '<='
                    } else {
                        return '='
                    }
                }

