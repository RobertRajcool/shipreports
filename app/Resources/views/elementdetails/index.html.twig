                        var data = {
                            symbolName: symbolName.val(),
                            symbolIndication: symbolIndication.val()
                        };
                        $.ajaxSetup({
                            global: false,
                            type:"POST",
                            url:"add_element_symbol",/*Routing.generate('add_element_symbol'),*/
