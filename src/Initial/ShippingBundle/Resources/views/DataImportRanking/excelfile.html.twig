{% extends template %}

{% block body %}
     {% block javascripts %}

          {% javascripts '@InitialShippingBundle/Resources/public/js/*' %}
          <script src="{{ asset_url }}"></script>
          {% endjavascripts %}
     {#  {% javascripts '@InitialShippingBundle/Resources/public/js/rules/*' %}
         <script src="{{ asset_url }}"></script>
         {% endjavascripts %}#}



      {% endblock %}

            <section>
                <div class="col-sm-12">
                    <div class="add_kpi_views_screen">
                          <form class="form-horizontal no-style" action="readfile_gearman_ranking" method="post" enctype="multipart/form-data">
                            <div class="kpi_basic_details">
                                <fieldset class="fieldset_box add_rule_form">
                                    <legend class="legend_name">Data import</legend>
                                    <div class="form-group">
                                        <label class="control-label col-xs-4">Select file</label>
                                        <div class="col-xs-8">
                                            <input type="file"  name="filename" id="fileid">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-4">Date of month</label>
                                        <div class="col-xs-8">
                                            {{ form_widget(form.dataofmonth) }}
                                        </div>
                                    </div>
                                </fieldset>
                            </div>

                            <div class="actions_submit">
                                <input type="submit" class="submit_btn" id="addsubmitid" value="Upload" />
                                <button class="cancel_btn" type="reset" value="Submit">Reset</button>
                            </div>
                          </form>
                    </div>
                </div>
            </section>
{% endblock %}