                                        $('<td class="month"></td>').appendTo('#praviousRowId'+inn);
                                    }

                                    if(j < data.yearMonthName.length-1)
                                    {
                                        var status = data.yearKpiColorArray[j+1][inn];

                                        if(color=='Green' && status=='Green' )
