                            $.ajax({
                                success: function(data)
                                {
                                    $('#area').highcharts().series[0].setData(data.changechartdata);
                                    $('#area').highcharts().series[0].update({ name: month+'-'+year});
                                    $('#currentid').text(month+'-'+year)
                                    $('#dataofmonth').val(currentdate)
