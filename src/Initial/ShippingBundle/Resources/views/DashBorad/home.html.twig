                                    if($('body').find('#resultLoading').attr('id') != 'resultLoading'){
                                        $('body').append('<div id="resultLoading" style="display:none"><div><img src="/images/ajax-loader.gif"><div>'+text+'</div></div><div class="bg"></div></div>');
                                    }

