                    $('#spantext').hide();

                    $('.yearViewContent').hide();

                    $('.yearView').click(function($e)
                    {
                        $e.preventDefault();
                        $('.wholeContent').hide();
                        $('.yearViewContent').show();
                    })

                    $('.fullYearView').click(function($e)
