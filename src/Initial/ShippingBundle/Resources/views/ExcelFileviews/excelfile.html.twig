{% extends 'base.html.twig' %}
{% block title %}Star Shipping Application{% endblock %}

{% block content %}
    <h1 align="center">Shipping Application</h1>

{% endblock %}
{% block body %}
     {% block javascripts %}

          {% javascripts '@InitialShippingBundle/Resources/public/js/*' %}
          <script src="{{ asset_url }}"></script>
          {% endjavascripts %}
{#
         {% javascripts '@InitialShippingBundle/Resources/public/js/rules/*' %}
         <script src="{{ asset_url }}"></script>
         {% endjavascripts %}#}



      {% endblock %}

      {% block stylesheets %}
          {% stylesheets '@InitialShippingBundle/Resources/public/css/*' %}
          <link rel="stylesheet" href="{{ asset_url }}" />
          {% endstylesheets %}
      {% endblock %}


    <div id="formcontent" align="center">
        <h2>Upload File Here</h2>

        <div class="modal" style="display: none">
            <div class="center">
                {% image '@InitialShippingBundle/Resources/public/img/loader5.gif' %}
                <img src="{{ asset_url }}" alt="Loader Image"/>
                {% endimage %}
            </div>
        </div>
        {#

        <div id="overlay">
            {% image '@InitialShippingBundle/Resources/public/img/ajax-loader.gif' %}
            <img src="{{ asset_url }}" alt="Loader Image"/>
            {% endimage %}
        </div>
#}
        <table>
            {{ form_start(form, {'attr': {'id': 'form_excel'}}) }}
            <tr><td>
                    {{ form_label(form.filename,'Choose File') }}</td><td>
                    {{ form_widget(form.filename,{'id' :'fileid'},{'label' :'Choose File'}, { 'attr': {'class': 'fileid1'} }) }}</td></tr><tr><td>
                    {{ form_label(form.dataofmonth,'Choose Month') }}</td><td>
                    {{ form_widget(form.dataofmonth) }}</td></tr><tr><td></td><td>
                    {{ form_widget(form.submit,{'id' :'submitid'}, { 'attr': {'class': 'submitclass'} }) }}
                    {{ form_widget(form.add,{'id' :'readingid'}, { 'attr': {'class': 'readingclass'} }) }}</td></tr>
            {{ form_end(form) }}
        </table>
    </div>
{% endblock %}